# =====================================
# CONFIGURATION DENTAFLOW
# =====================================
# Copiez ce fichier en .env et adaptez les valeurs

# --- Environnement ---
NODE_ENV=development
APP_PORT=3000
APP_URL=http://localhost:3000

# --- Base de données ---
DB_HOST=postgres
DB_PORT=5432
DB_NAME=dentaflow_dev
DB_USER=postgres
DB_PASSWORD=change_this_password

# --- Redis ---
REDIS_PORT=6379

# --- Sécurité ---
JWT_SECRET=your_super_secret_jwt_key_change_in_production
JWT_EXPIRES_IN=24h

# --- SonarQube / CI-CD ---
SONAR_TOKEN=your_sonarcloud_token_here

# --- API Externes ---
API_3D_URL=https://api.render3d.example.com
API_3D_KEY=your_api_key_here

